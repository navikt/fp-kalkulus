<?xml version="1.0" encoding="UTF-8"?>
<project xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://maven.apache.org/POM/4.0.0"
	xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
	<modelVersion>4.0.0</modelVersion>

	<groupId>no.nav.folketrygdloven.kalkulus</groupId>
	<artifactId>ft-kalkulus</artifactId>
	<packaging>pom</packaging>
	<name>Kalkulus :: Root</name>
	<version>${revision}${sha1}${changelist}</version>

	<modules>
		<module>kalkulator</module>
	</modules>

	<properties>
		<revision>1.1</revision>
		<sha1 />
		<changelist>-SNAPSHOT</changelist>
		<java.version>11</java.version>
		<maven.compiler.source>${java.version}</maven.compiler.source>
		<maven.compiler.target>${java.version}</maven.compiler.target>
		<project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
		<project.reporting.outputEncoding>UTF-8</project.reporting.outputEncoding>

		<prometheus.version>0.8.0</prometheus.version>

		<felles.version>1.4.2-20191218110056-fa5b90a</felles.version>

		<kontrakter.version>5.1_20200114070256_f09deef</kontrakter.version>
	</properties>

	<prerequisites>
		<!-- deprecated i maven3, men trengs av versions plugin -->
		<maven>3.6.1</maven>
	</prerequisites>

	<!-- NB: Unngå å put scope (test, provided) i dependency management. Det
		har uheldige virkninger ved bruk av import og dependency (bruk composition
		over arv) da de overstyrer andre scope definisjoner. Sett det heller der
		det brukes. -->
	<dependencyManagement>
		<dependencies>
			<!-- Felles biblioteker - Importerte avhengigheter -->
			<dependency>
				<groupId>no.nav.foreldrepenger.felles</groupId>
				<artifactId>felles</artifactId>
				<version>${felles.version}</version>
				<scope>import</scope>
				<type>pom</type>
			</dependency>
			<dependency>
				<groupId>no.nav.foreldrepenger.kontrakter</groupId>
				<artifactId>fp-kontrakter-root</artifactId>
				<version>${kontrakter.version}</version>
				<scope>import</scope>
				<type>pom</type>
			</dependency>
			<dependency>
				<groupId>org.jboss.spec.javax.annotation</groupId>
				<artifactId>jboss-annotations-api_1.3_spec</artifactId>
				<version>2.0.1.Final</version>
			</dependency>
			<dependency>
				<groupId>no.nav.folketrygdloven</groupId>
				<artifactId>beregningsgrunnlag-regelmodell</artifactId>
				<version>2.1-20200121130215-ad804eb</version>
			</dependency>

			<!-- Prosjektets egne biblioteker -->
			<dependency>
				<groupId>no.nav.foreldrepenger.felles</groupId>
				<artifactId>felles-prosesstask</artifactId>
				<version>${felles.version}</version>
			</dependency>

			<!-- Prosjektets egne TEST AVHENGIGHETER -->
			<!-- NB: Definer scope der det brukes, ikke her. -->

			<dependency>
				<groupId>no.nav.foreldrepenger</groupId>
				<artifactId>web-apidoc</artifactId>
				<version>${project.version}</version>
			</dependency>
			<dependency>
				<groupId>no.nav.foreldrepenger</groupId>
				<artifactId>web-webapp</artifactId>
				<type>jar</type>
				<version>${project.version}</version>
			</dependency>
			<dependency>
				<groupId>no.nav.foreldrepenger</groupId>
				<artifactId>dokumentbestiller</artifactId>
				<version>${project.version}</version>
			</dependency>
			<dependency>
				<groupId>no.nav.foreldrepenger</groupId>
				<artifactId>risikoklassifisering</artifactId>
				<version>${project.version}</version>
			</dependency>
			<dependency>
				<groupId>no.nav.foreldrepenger</groupId>
				<artifactId>dokumentbestiller-xsd</artifactId>
				<version>${project.version}</version>
			</dependency>
			<dependency>
				<groupId>no.nav.foreldrepenger</groupId>
				<artifactId>okonomistotte</artifactId>
				<version>${project.version}</version>
			</dependency>
			<dependency>
				<groupId>no.nav.foreldrepenger</groupId>
				<artifactId>tilbakekreving</artifactId>
				<version>${project.version}</version>
			</dependency>
			<dependency>
				<groupId>no.nav.foreldrepenger</groupId>
				<artifactId>opptjeningsvilkar</artifactId>
				<version>${project.version}</version>
			</dependency>
			<dependency>
				<groupId>no.nav.foreldrepenger</groupId>
				<artifactId>opptjeningsvilkar-regelmodell</artifactId>
				<version>${project.version}</version>
			</dependency>
			<dependency>
				<groupId>no.nav.foreldrepenger.kontrakter</groupId>
				<artifactId>okonomistotte-v1</artifactId>
				<version>2.0_20190311110043_ddc2d1e</version>
				<classifier>jaxb</classifier>
			</dependency>
			<dependency>
				<groupId>no.nav.foreldrepenger.kontrakter</groupId>
				<artifactId>grensesnittavstemming-v1</artifactId>
				<version>2.3_20180507114256_2ed2065</version>
				<classifier>jaxb</classifier>
			</dependency>
			<dependency>
				<groupId>no.nav.foreldrepenger.kontrakter</groupId>
				<artifactId>dokumentbestiller-v1</artifactId>
				<version>2.0_20190816101820_003f3bb</version>
				<classifier>jaxb</classifier>
			</dependency>
			<dependency>
				<groupId>no.nav.foreldrepenger</groupId>
				<artifactId>behandlingslager-testutilities</artifactId>
				<version>${project.version}</version>
			</dependency>
			<dependency>
				<groupId>no.nav.foreldrepenger</groupId>
				<artifactId>dokumentarkiv</artifactId>
				<version>${project.version}</version>
			</dependency>

			<!-- EKSTRA TEST AVHENGIGHETER FOR LOKAL WEBSERVER -->
			<!-- NB: Definer scope der det brukes, ikke her. -->
			<dependency>
				<groupId>net.logstash.logback</groupId>
				<artifactId>logstash-logback-encoder</artifactId>
				<version>4.10</version>
			</dependency>
			<dependency>
				<groupId>com.papertrailapp</groupId>
				<artifactId>logback-syslog4j</artifactId>
				<version>1.0.0</version>
			</dependency>

			<!-- Transitive avhengighet resolution for å sikre bestemt versjon -->
			<dependency>
				<groupId>joda-time</groupId>
				<artifactId>joda-time</artifactId>
				<version>2.9.9</version>
			</dependency>
			<dependency>
				<groupId>org.jasypt</groupId>
				<artifactId>jasypt</artifactId>
				<version>1.9.2</version>
			</dependency>
			<dependency>
				<groupId>io.swagger</groupId>
				<artifactId>swagger-compat-spec-parser</artifactId>
				<version>1.0.48</version>
			</dependency>
			<dependency>
				<groupId>io.javaslang</groupId>
				<artifactId>javaslang</artifactId>
				<version>2.0.6</version>
			</dependency>
			<dependency>
				<groupId>io.swagger.core.v3</groupId>
				<artifactId>swagger-jaxrs2</artifactId>
				<version>2.1.0</version>
			</dependency>
			<dependency>
				<groupId>io.swagger.core.v3</groupId>
				<artifactId>swagger-core</artifactId>
				<version>2.1.0</version>
			</dependency>
			<dependency>
				<groupId>io.swagger.core.v3</groupId>
				<artifactId>swagger-annotations</artifactId>
				<version>2.1.0</version>
			</dependency>
			<dependency>
				<groupId>io.github.swagger2markup</groupId>
				<artifactId>markup-document-builder</artifactId>
				<version>1.1.2</version>
			</dependency>
			<dependency>
				<groupId>org.owasp.encoder</groupId>
				<artifactId>encoder</artifactId>
				<version>1.2.2</version>
			</dependency>
			<dependency>
				<groupId>org.assertj</groupId>
				<artifactId>assertj-core</artifactId>
				<version>3.8.0</version>
			</dependency>
			<dependency>
				<groupId>junit</groupId>
				<artifactId>junit</artifactId>
				<version>4.12</version>
			</dependency>
			<dependency>
				<groupId>org.glassfish.jaxb</groupId>
				<artifactId>jaxb-runtime</artifactId>
				<version>2.4.0-b180830.0438</version>
			</dependency>
			<dependency>
				<groupId>javax.xml.bind</groupId>
				<artifactId>jaxb-api</artifactId>
				<version>2.4.0-b180830.0359</version>
			</dependency>
			<dependency>
				<groupId>com.google.code.findbugs</groupId>
				<artifactId>jsr305</artifactId>
				<version>3.0.2</version>
			</dependency>

			<!-- Java11 -->
			<dependency>
				<groupId>org.jboss.classfilewriter</groupId>
				<artifactId>jboss-classfilewriter</artifactId>
				<version>1.2.3.Final</version>
			</dependency>
			<dependency>
				<groupId>org.javassist</groupId>
				<artifactId>javassist</artifactId>
				<version>3.23.1-GA</version>
			</dependency>
			<dependency>
				<groupId>org.apache.santuario</groupId>
				<artifactId>xmlsec</artifactId>
				<version>2.1.2</version>
			</dependency>

			<!-- bean validation -->
			<dependency>
				<groupId>org.glassfish</groupId>
				<artifactId>javax.el</artifactId>
				<version>3.0.1-b11</version>
			</dependency>

		</dependencies>
	</dependencyManagement>

	<build>
		<pluginManagement>
			<plugins>
				<plugin>
					<groupId>org.jboss.jandex</groupId>
					<artifactId>jandex-maven-plugin</artifactId>
					<version>1.0.7</version>
					<executions>
						<execution>
							<id>make-index</id>
							<goals>
								<goal>jandex</goal>
							</goals>
							<configuration>
								<!-- Nothing needed here for simple cases -->
							</configuration>
						</execution>
					</executions>
				</plugin>
				<plugin>
					<groupId>org.jacoco</groupId>
					<artifactId>jacoco-maven-plugin</artifactId>
					<version>0.8.5</version>
					<executions>
						<execution>
						    <goals>
						      <goal>prepare-agent</goal>
						    </goals>
						</execution>
					</executions>
			    </plugin>
				<plugin>
					<groupId>org.apache.maven.plugins</groupId>
					<artifactId>maven-deploy-plugin</artifactId>
					<version>2.8.2</version>
				</plugin>

				<plugin>
					<groupId>org.asciidoctor</groupId>
					<artifactId>asciidoctor-maven-plugin</artifactId>
					<version>1.6.0</version>
				</plugin>
				<plugin>
					<groupId>org.codehaus.mojo</groupId>
					<artifactId>buildnumber-maven-plugin</artifactId>
					<version>1.4</version>
				</plugin>
				<plugin>
					<groupId>org.apache.maven.plugins</groupId>
					<artifactId>maven-clean-plugin</artifactId>
					<version>3.1.0</version>
				</plugin>
				<plugin>
					<groupId>org.apache.maven.plugins</groupId>
					<artifactId>maven-compiler-plugin</artifactId>
					<version>3.8.1</version>
					<configuration>
						<source>${java.version}</source>
						<target>${java.version}</target>
						<encoding>UTF-8</encoding>
						<release>${java.version}</release>
					</configuration>
				</plugin>
				<plugin>
					<groupId>org.apache.maven.plugins</groupId>
					<artifactId>maven-install-plugin</artifactId>
					<version>2.5.2</version>
				</plugin>
				<plugin>
					<groupId>org.apache.maven.plugins</groupId>
					<artifactId>maven-jar-plugin</artifactId>
					<version>3.2.0</version>
				</plugin>
				<plugin>
					<groupId>org.codehaus.mojo</groupId>
					<artifactId>exec-maven-plugin</artifactId>
					<version>1.6.0</version>
				</plugin>
				<plugin>
					<groupId>org.apache.maven.plugins</groupId>
					<artifactId>maven-remote-resources-plugin</artifactId>
					<version>1.6.0</version>
				</plugin>
				<plugin>
					<groupId>org.codehaus.mojo</groupId>
					<artifactId>flatten-maven-plugin</artifactId>
					<version>1.1.0</version>
				</plugin>
                <plugin>
					<groupId>org.sonarsource.scanner.maven</groupId>
					<artifactId>sonar-maven-plugin</artifactId>
					<version>3.7.0.1746</version>
				</plugin>
			</plugins>
		</pluginManagement>

		<plugins>
			<plugin>
				<groupId>org.jboss.jandex</groupId>
				<artifactId>jandex-maven-plugin</artifactId>
				<executions>
					<execution>
						<id>make-index</id>
						<goals>
							<goal>jandex</goal>
						</goals>
						<configuration>
							<!-- Nothing needed here for simple cases -->
						</configuration>
					</execution>
				</executions>
			</plugin>
			<plugin>
				<groupId>org.codehaus.mojo</groupId>
				<artifactId>flatten-maven-plugin</artifactId>
				<configuration>
					<flattenMode>bom</flattenMode>
					<flattenedPomFilename>.flattened</flattenedPomFilename>
				</configuration>
				<executions>
					<execution>
						<id>flatten</id>
						<phase>process-resources</phase>
						<goals>
							<goal>flatten</goal>
						</goals>
					</execution>
					<execution>
						<id>flatten.clean</id>
						<phase>clean</phase>
						<goals>
							<goal>clean</goal>
						</goals>
					</execution>
				</executions>
			</plugin>
			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-source-plugin</artifactId>
				<version>3.2.1</version>
				<executions>
					<execution>
						<id>attach-sources</id>
						<goals>
							<goal>jar</goal>
						</goals>
					</execution>
				</executions>
			</plugin>
            <plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-deploy-plugin</artifactId>
				<configuration>
					<deployAtEnd>true</deployAtEnd>
					<skip>true</skip>
				</configuration>
			</plugin>
		</plugins>
	</build>

	<scm>
		<connection>scm:git:https://github.com/navikt/ft-kalkulus.git</connection>
		<developerConnection>scm:git:https://github.com/navikt/ft-kalkulus.git</developerConnection>
		<url>https://github.com/navikt/ft-kalkulus</url>
		<tag>HEAD</tag>
	</scm>

    <distributionManagement>
        <repository>
            <id>github</id>
            <name>Github navikt Maven Packages</name>
            <url>https://maven.pkg.github.com/navikt/ft-kalkulus</url>
        </repository>
    </distributionManagement>

</project>
